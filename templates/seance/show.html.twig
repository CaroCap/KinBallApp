{% extends 'base.html.twig' %}

{% block title %}Seance{% endblock %}

{% block body %}
    <h1>Seance</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ seance.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ seance.title }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ seance.description }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ seance.start ? seance.start|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ seance.end ? seance.end|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ seance.numero }}</td>
            </tr>
            <tr>
                <th>Rue</th>
                <td>{{ seance.rue }}</td>
            </tr>
            <tr>
                <th>CodePostal</th>
                <td>{{ seance.codePostal }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ seance.ville }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_seance_index') }}">back to list</a>

    <a href="{{ path('app_seance_edit', {'id': seance.id}) }}">edit</a>

    {{ include('seance/_delete_form.html.twig') }}
{% endblock %}
